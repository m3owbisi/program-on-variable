#include <stdio.h>
#include<conio.h>
void main()
{
	int a, b;
	clrscr();
	printf(“Enter the value of a: ”);
	scanf(“%d”,&a);
	printf(“Enter the value of b: ”);
	scanf(“%d”,&b);
	printf(“Before Swapping the value of a & b\n”);
	printf(“The value of a is %d and the value of b is %d\n”,a,b);
	printf();
	a = a + b;
	b = a – b;
	a = a – b;
	printf();
	printf(“After Swapping the value of a & b\n”);
	printf(“The value of a is %d and the value of b is %d\n”,a,b);
	getch();
	return 0;
}
